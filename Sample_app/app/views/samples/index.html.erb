<h1>投稿フォーム</h1>
<%= form_with model: @sample, local: true do |f| %>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>body</h4>
<%= f.text_area :body %>

<%= f.submit "投稿" %>

<% end %>

<h1>投稿一覧</h1>
<% @samples.each do |sample| %>
<p>title:<%=sample.title %></p>
<p>body:<%=sample.body %></p>

<% end %>